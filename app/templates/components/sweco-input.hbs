{{sweco-label
    inputId=inputId
    label=label
    icon=icon
}}
{{input
    id=inputId
    value=model
    type=type
    class=customClass
    required=required
    focus-out='removeClass'
    focus-in='setClass'
    name=name
    disabled=disabled
}}
{{sweco-help
    helpUrl=helpUrl
    help=help
}}